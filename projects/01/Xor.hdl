/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not (in=a, out=notA);
    And (a=notA, b=b, out=BAndNotA);
    Not (in=b, out=notB);
    And (a=a, b=notB, out=AAndNotB);
    Or (a=BAndNotA, b=AAndNotB, out=out);
}